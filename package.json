    "@types/eslint": "^9.6.0",
    "eslint": "^9.9.0",
    "eslint-plugin-svelte": "^2.43.0",
    "globals": "^15.9.0",
    "svelte": "5.0.0-next.226",
    "svelte-check": "^3.8.5",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.2.0",
    "vite": "^5.4.1"
  },
  "type": "module",
  "dependencies": {
    "@auth/core": "^0.34.2",
    "@auth/sveltekit": "^1.4.2",
    "@builder.io/sdk-svelte": "^2.0.3",
